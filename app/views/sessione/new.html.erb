<h1>Log in</h1>

<%= form_tag ("sessione/create") do %>
  <div class="field">
    <%= label_tag :nickname %><br />
    <%= text_field_tag :nickname, params[:nickname] %>
  </div>
  <div cless="field">
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </div>
  <div class="action"><%= submit_tag "Log In" %></div>
<% end %>


